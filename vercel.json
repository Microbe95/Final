{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/manifest.json",
      "headers": {
        "Content-Type": "application/manifest+json"
      }
    },
    {
      "src": "/sw.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    }
  ],
  "env": {
    "JWT_SECRET": "@jwt-secret",
    "NEXT_PUBLIC_API_URL": "@next-public-api-url"
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 10
    }
  }
} 