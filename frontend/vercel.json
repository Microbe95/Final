{
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["icn1"],
  "functions": {
    "app/**/*.tsx": {
      "maxDuration": 30
    }
  },
  "env": {
    "NEXT_PUBLIC_API_URL": "http://localhost:8080",
    "NEXT_PUBLIC_API_BASE_URL": "http://localhost:8080/api/v1",
    "GATEWAY_URL": "http://localhost:8080"
  }
}
